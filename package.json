{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "server.ts",
  "scripts": {
    "build": "npm install && npm install --prefix ./frontend && npm run build --prefix ./frontend && tsc --build tsconfig.json",
    "start": "nodemon --exec npx ts-node --esm backend/src/server.ts",
    "client": "npm run dev --prefix ./frontend",
    "dev": "concurrently \"npm start \" \"npm run client\"",
    "data:import": "npx ts-node --esm  backend/src/seeder.ts",
    "data:destroy": "npx ts-node --esm  backend/src/seeder.ts -del"
  },
  "keywords": [],
  "author": "Srinivas Nahak",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.2",
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.2"
  }
}
